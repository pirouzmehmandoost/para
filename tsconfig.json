{
  "compilerOptions": {
    "paths": {
      "@/*": ["./src/*"],
      "@public/*": ["./public/*"],
      "@utils/*": ["./src/lib/utils/*"],
      "@configs/*": ["./src/lib/configs/*"],
      "@stores/*": ["./src/app/stores/*"],
      "@projects/*": ["./src/app/projects/*"],
      "@components/*": ["./src/app/components/*"],
      "@three/*": ["./src/app/components/three/*"],
      "@ui/*": ["./src/app/components/ui/*"],
    },
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    ".next/dev/types/**/*.ts",
    "**/*.mts",
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}